{
  "name": "typescript-express-api",
  "version": "1.0.0",
  "description": "ts-express-api Scaffolding babel-with-typescript-express-api",
  "main": "index.js",
  "author": "jackieli",
  "scripts": {
    "dev": "cross-env DEV_ENV=true nodemon index.js",
    "start": "node index.js",
